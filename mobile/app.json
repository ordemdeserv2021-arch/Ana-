{
  "expo": {
    "name": "Ana",
    "slug": "ana-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ana.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.ana.mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que Ana acesse sua câmera para capturar fotos biométricas"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir que Ana acesse suas fotos",
          "cameraPermission": "Permitir que Ana acesse sua câmera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "305e841c-1734-4ce3-bb8b-7658830d593e"
      }
    }
  }
}
